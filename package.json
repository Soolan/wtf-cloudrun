{
  "name": "wtf",
  "version": "sprint-69",
  "author": "Sohail Salehi <info@soolan.com> (https://github.com/soolan)",
  "scripts": {
    "ng": "ng",
    "start:dashboard": "ng serve dashboard --open --port 4200",
    "start:website": "ng serve website --open --port 4201",
    "build:dashboard": "ng build --project=dashboard",
    "build:website": "ng build --project=website ",
    "watch:dashboard": "ng build --project=dashboard --watch --configuration development",
    "watch:website": "ng build --project=website --watch --configuration development",
    "test:dashboard": "ng test dashboard",
    "test:website": "ng test website",
    "build:ssr:dashboard": "ng build --project=dashboard && ng run dashboard:serve",
    "build:ssr:website": "ng build --project=website && ng run website:serve",
    "dev:ssr:dashboard": "ng run dashboard:serve-ssr",
    "dev:ssr:website": "ng run website:serve-ssr",
    "prerender:dashboard": "ng run dashboard:prerender",
    "prerender:website": "ng run website:prerender",
    "serve:ssr:dashboard": "node dist/dashboard/server/server.mjs",
    "serve:ssr:website": "node dist/website/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.6",
    "@angular/cdk": "^20.1.5",
    "@angular/common": "^20.1.6",
    "@angular/compiler": "^20.1.6",
    "@angular/core": "^20.1.6",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.1.6",
    "@angular/material": "^20.1.5",
    "@angular/platform-browser": "^20.1.6",
    "@angular/platform-browser-dynamic": "^20.1.6",
    "@angular/platform-server": "^20.1.6",
    "@angular/router": "^20.1.6",
    "@angular/service-worker": "^20.1.6",
    "@angular/ssr": "^20.1.5",
    "bpmn-js": "^18.6.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase": "^12.0.0",
    "ngx-image-cropper": "^9.1.5",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.6",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^18.18.0",
    "@types/qrcode": "^1.5.5",
    "jasmine-core": "~5.7.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  },
  "type": "module"
}
