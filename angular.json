{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/website",
      "sourceRoot": "projects/website/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/website",
            "index": "projects/website/src/index.html",
            "browser": "projects/website/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/website/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/website/public"
              },
              {
                "glob": "**/*",
                "input": "projects/shared/assets",
                "output": "/assets"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/shared/assets/styles"
              ]
            },
            "styles": [
              "projects/website/src/styles.scss",
              "node_modules/bpmn-js/dist/assets/diagram-js.css",
              "node_modules/bpmn-js/dist/assets/bpmn-font/css/bpmn.css"
            ],
            "scripts": [],
            "server": "projects/website/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/website/src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8MB",
                  "maximumError": "10MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1MB",
                  "maximumError": "2MB"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "projects/website/ngsw-config.json",
              "optimization": {
                "styles": {
                  "inlineCritical": false
                }
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/website/src/environments/environment.ts",
                  "with": "projects/website/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "website:build:production"
            },
            "development": {
              "buildTarget": "website:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/website/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/website/public"
              }
            ],
            "styles": [
              "projects/website/src/styles.scss",
              "projects/shared/styles/material-theme.scss"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular/ssr:server",
          "options": {
            "outputPath": "dist/website/server",
            "main": "projects/website/src/main.server.ts",
            "tsConfig": "projects/website/tsconfig.server.json"
          }
        },
        "serve-ssr": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/website/server",
            "main": "projects/website/src/main.server.ts",
            "tsConfig": "projects/website/tsconfig.server.json"
          }
        },
        "prerender": {
          "builder": "@angular/ssr:prerender",
          "options": {
            "browserTarget": "website:build",
            "serverTarget": "website:server"
          }
        }
      }
    },
    "dashboard": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/dashboard",
